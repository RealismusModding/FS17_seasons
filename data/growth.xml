<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<growth>
    <annotation>Copyright (c) Realismus Modding, 2017</annotation>
    <contributors>theSeb</contributors>
    <version>2</version>

    <defaultFruits>
        <defaultFruit fruitName="barley" maxSprayGrowthState="4"/>
        <defaultFruit fruitName="wheat" maxSprayGrowthState="4"/>
        <defaultFruit fruitName="rape" maxSprayGrowthState="4"/>
        <defaultFruit fruitName="maize" maxSprayGrowthState="4"/>
        <defaultFruit fruitName="soybean" maxSprayGrowthState="4"/>
        <defaultFruit fruitName="sunflower" maxSprayGrowthState="4"/>
        <defaultFruit fruitName="potato" maxSprayGrowthState="4"/>
        <defaultFruit fruitName="sugarBeet" maxSprayGrowthState="4"/>
        <defaultFruit fruitName="poplar" maxSprayGrowthState="4"/>
        <defaultFruit fruitName="grass" maxSprayGrowthState="4"/>
        <defaultFruit fruitName="oilseedRadish" />
        <defaultFruit fruitName="dryGrass" />
        <defaultFruit fruitName="sugarCane" />
    </defaultFruits>
    <growthTransitions>
        <gt growthTransitionNum="1"> <!--late winter to early spring-->
            <fruit fruitName="barley"  incrementByOneRange="1-2"/>
            <fruit fruitName="wheat"   incrementByOneRange="1-2"/>
            <fruit fruitName="rape"    incrementByOneRange="1-2"/>
            <fruit fruitName="poplar" incrementByOneRange="1-MAX"/>
            <fruit fruitName="oilseedRadish" incrementByOneRange="1-MAX"/>
            <fruit fruitName="maize" setRange="1" setTo="CUT"/>
            <fruit fruitName="potato" setRange="1" setTo="CUT"/>
            <fruit fruitName="sugarBeet" setRange="1" setTo="CUT"/>
        </gt>
        <gt growthTransitionNum="2"> <!--early spring to mid spring-->
            <fruit fruitName="barley"  incrementByOneRange="1-3"/>
            <fruit fruitName="wheat"   incrementByOneRange="1-3"/>
            <fruit fruitName="rape"    incrementByOneRange="1-3"/>
            <fruit fruitName="maize" incrementByOneRange="1"/>
            <fruit fruitName="soybean" setRange="1" setTo="CUT"/>
            <fruit fruitName="sunflower" setRange="1" setTo="CUT"/>
            <fruit fruitName="potato"    incrementByOneRange="1"/>
            <fruit fruitName="sugarBeet" incrementByOneRange="1"/>
            <fruit fruitName="poplar" setRange="CUT" setTo="1" incrementByOneRange="1-MAX"/>
            <fruit fruitName="grass" incrementByOneRange="1" setRange="2" setTo="3"/>
            <fruit fruitName="oilseedRadish" incrementByOneRange="1-MAX"/>
        </gt>
        <gt growthTransitionNum="3"> <!--mid spring to late spring-->
            <fruit fruitName="barley"  incrementByOneRange="1-3"/>
            <fruit fruitName="wheat"   incrementByOneRange="1-3"/>
            <fruit fruitName="rape"    incrementByOneRange="1-3"/>
            <fruit fruitName="maize" incrementByOneRange="1-2"/>
            <fruit fruitName="soybean" incrementByOneRange="1"/>
            <fruit fruitName="sunflower" incrementByOneRange="1"/>
            <fruit fruitName="potato"    incrementByOneRange="1-2"/>
            <fruit fruitName="sugarBeet" incrementByOneRange="1-2"/>
            <fruit fruitName="oilseedRadish" incrementByOneRange="1-MAX"/>
        </gt>
        <gt growthTransitionNum="4"> <!--late spring to early summer-->
            <fruit fruitName="barley"  incrementByOneRange="1-4"/>
            <fruit fruitName="wheat"   incrementByOneRange="1-4"/>
            <fruit fruitName="rape"    incrementByOneRange="1-4"/>
            <fruit fruitName="maize" incrementByOneRange="1-3"/>
            <fruit fruitName="soybean" incrementByOneRange="1-2"/>
            <fruit fruitName="sunflower" incrementByOneRange="1-2"/>
            <fruit fruitName="potato"    incrementByOneRange="1-3"/>
            <fruit fruitName="sugarBeet" incrementByOneRange="1-3"/>
            <fruit fruitName="poplar" setRange="CUT" setTo="1" incrementByOneRange="1-MAX"/>
            <fruit fruitName="grass" incrementByOneRange="1-MAX"/>
            <fruit fruitName="oilseedRadish" incrementByOneRange="1-MAX"/>
        </gt>
        <gt growthTransitionNum="5"> <!--early summer to mid summer-->
            <fruit fruitName="barley"  incrementByOneRange="2-5" setRange="1" setTo="CUT"/>
            <fruit fruitName="wheat"   incrementByOneRange="2-5" setRange="1" setTo="CUT"/>
            <fruit fruitName="rape"    incrementByOneRange="2-5" setRange="1" setTo="CUT"/>
            <fruit fruitName="maize" incrementByOneRange="1-4"/>
            <fruit fruitName="soybean" incrementByOneRange="1-3"/>
            <fruit fruitName="sunflower" incrementByOneRange="2-3" setRange="1" setTo="CUT"/>
            <fruit fruitName="potato"    incrementByOneRange="2-4" setRange="1" setTo="CUT"/>
            <fruit fruitName="sugarBeet" incrementByOneRange="2-4" setRange="1" setTo="CUT"/>
        </gt>
        <gt growthTransitionNum="6"> <!--mid summer to late summer-->
            <fruit fruitName="barley"  incrementByOneRange="6" incrementByRange="3-5" incrementBy="2" setRange="1"  setTo="CUT"/>
            <fruit fruitName="wheat"   incrementByOneRange="6" incrementByRange="3-5" incrementBy="2" setRange="1"  setTo="CUT"/>
            <fruit fruitName="rape"    incrementByOneRange="6" incrementByRange="3-5" incrementBy="2" setRange="1"  setTo="CUT"/>
            <fruit fruitName="maize" incrementByRange="2-5" incrementBy="2" setRange="1"  setTo="CUT"/>
            <fruit fruitName="soybean" incrementByRange="2-4" incrementBy="2" setRange="1"  setTo="CUT"/>
            <fruit fruitName="sunflower" incrementByRange="3-4" incrementBy="2" setRange="1"  setTo="CUT"/>
            <fruit fruitName="potato"    incrementByOneRange="3-5" setRange="1" setTo="CUT"/>
            <fruit fruitName="sugarBeet" incrementByOneRange="3-5" setRange="1" setTo="CUT"/>
            <fruit fruitName="grass" incrementByOneRange="2-MAX"/>
        </gt>
        <gt growthTransitionNum="7"> <!--late summer to early autumn-->
            <fruit fruitName="barley"  incrementByOneRange="5-6" setRange="1"  setTo="CUT"/>
            <fruit fruitName="wheat"   incrementByOneRange="5-6" setRange="1"  setTo="CUT"/>
            <fruit fruitName="rape"    incrementByOneRange="5-6" />
            <fruit fruitName="maize" incrementByOneRange="4-6" setRange="1"  setTo="CUT"/>
            <fruit fruitName="soybean" incrementByOneRange="4-6" setRange="1"  setTo="CUT"/>
            <fruit fruitName="sunflower" incrementByOneRange="5-6" setRange="1"  setTo="CUT"/>
            <fruit fruitName="potato"    incrementByOneRange="4-5" setRange="1"  setTo="CUT"/>
            <fruit fruitName="sugarBeet" incrementByOneRange="4-6" setRange="1"  setTo="CUT"/>
            <fruit fruitName="poplar" setRange="CUT" setTo="1" incrementByOneRange="1-MAX"/>
            <fruit fruitName="grass" incrementByOneRange="1-MAX"/>
            <fruit fruitName="oilseedRadish" incrementByOneRange="1-MAX"/>
        </gt>
        <gt growthTransitionNum="8"> <!--early autumn to mid autumn-->
            <fruit fruitName="barley"  incrementByOneRange="1-6"/>
            <fruit fruitName="wheat"   incrementByOneRange="1-6"/>
            <fruit fruitName="rape"    incrementByOneRange="1-6"/>
            <fruit fruitName="maize" incrementByOneRange="5-6" setRange="1"  setTo="CUT"/>
            <fruit fruitName="soybean" incrementByOneRange="5-6" setRange="1"  setTo="CUT"/>
            <fruit fruitName="sunflower" incrementByOneRange="6" setRange="1"  setTo="CUT"/>
            <fruit fruitName="potato"    incrementByOneRange="5" setRange="1"  setTo="CUT"/>
            <fruit fruitName="sugarBeet" incrementByOneRange="5-6" setRange="1"  setTo="CUT"/>
            <fruit fruitName="oilseedRadish" incrementByOneRange="1-MAX"/>
        </gt>
        <gt growthTransitionNum="9"> <!--mid autumn to late autumn-->
            <fruit fruitName="barley"  incrementByOneRange="1" />
            <fruit fruitName="wheat"   incrementByOneRange="1" />
            <fruit fruitName="rape"    incrementByOneRange="1" />
            <fruit fruitName="maize" incrementByOneRange="6" setRange="1"  setTo="CUT"/>
            <fruit fruitName="soybean" incrementByOneRange="6" setRange="1"  setTo="CUT"/>

            <fruit fruitName="sunflower" setRange="1"  setTo="CUT"/>
            <fruit fruitName="potato"    setRange="1"  setTo="CUT"/>
            <fruit fruitName="sugarBeet" incrementByOneRange="6" setRange="1"  setTo="CUT"/>
            <fruit fruitName="oilseedRadish" incrementByOneRange="1-MAX"/>
        </gt>
        <gt growthTransitionNum="10"> <!--late autumn to early winter-->
            <fruit fruitName="barley"  setRange="4-MAX" setTo="WITHERED"/>
            <fruit fruitName="wheat"   setRange="4-MAX" setTo="WITHERED"/>
            <fruit fruitName="rape"    setRange="4-MAX" setTo="WITHERED"/>
            <fruit fruitName="maize" setRange="1"  setTo="CUT" incrementByOneRange="7"/>
            <fruit fruitName="soybean" setRange="1" setTo="CUT" incrementByOneRange="7"/>
            <fruit fruitName="sunflower" setRange="1" setTo="CUT" incrementByOneRange="7"/>
            <fruit fruitName="potato" setRange="1" setTo="CUT" incrementByOneRange="6"/>
            <fruit fruitName="sugarBeet" setRange="1" setTo="CUT" incrementByOneRange="7"/>
            <fruit fruitName="grass" setRange="1-MAX" setTo="2"/>
        </gt>
        <gt growthTransitionNum="11"/> <!--early winter to mid winter-->
        <gt growthTransitionNum="12"/> <!--mid winter to late winter-->
        <gt growthTransitionNum="FIRST_LOAD_TRANSITION">
            <fruit fruitName="barley" setRange="1-MAX" setTo="CUT"/>
            <fruit fruitName="wheat" setRange="1-MAX" setTo="CUT"/>
            <fruit fruitName="rape" setRange="1-MAX" setTo="CUT"/>
            <fruit fruitName="maize" setRange="1-MAX" setTo="CUT"/>
            <fruit fruitName="soybean" setRange="1-MAX" setTo="CUT"/>
            <fruit fruitName="sunflower" setRange="1-MAX" setTo="CUT"/>
            <fruit fruitName="potato" setRange="1-MAX" setTo="CUT"/>
            <fruit fruitName="sugarBeet" setRange="1-MAX" setTo="CUT"/>
            <fruit fruitName="poplar" setRange="1-MAX" setTo="CUT"/>
            <fruit fruitName="grass" setRange="1-MAX" setTo="2"/>
            <fruit fruitName="oilseedRadish" setRange="1-MAX" setTo="CUT"/>
        </gt>
    </growthTransitions>
    <germination>
        <fruit fruitName="barley" germinateTemp="5" />
        <fruit fruitName="wheat" germinateTemp="5" />
        <fruit fruitName="rape" germinateTemp="5" />
        <fruit fruitName="maize" germinateTemp="10" />
        <fruit fruitName="soybean" germinateTemp="10" />
        <fruit fruitName="sunflower" germinateTemp="7" />
        <fruit fruitName="potato" germinateTemp="5" />
        <fruit fruitName="sugarBeet" germinateTemp="5" />
        <fruit fruitName="grass" germinateTemp="3" />
        <fruit fruitName="oilseedRadish" germinateTemp="7" />
        <fruit fruitName="poplar" germinateTemp="5" />
        <fruit fruitName="sugarCane" germinateTemp="12" />
    </germination>
</growth>
